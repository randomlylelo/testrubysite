<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <div>User <%= user.id %> </div>
    <li>
      <a href="<%= user_path(user) %>">
        Name: <%= user.name %>
      </a>
    </li>
    <li>
      Email: <%= user.email %>
     </li>
     <br />
  <% end %>
</ul>

<%= link_to "New User", new_user_path %>